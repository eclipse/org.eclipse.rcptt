   <extension
         point="org.eclipse.rcptt.ecl.core.listenSource">
      <listenSource
            class="org.eclipse.rcptt.ecl.platform.ErrorLogListenSource"
            name="errorLog">
      </listenSource>
   </extension>
</plugin>
