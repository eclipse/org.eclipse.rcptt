<plugin>
   <extension
         point="org.eclipse.emf.ecore.generated_package">
 	<package
            class="org.eclipse.rcptt.ecl.platform.objects.ObjectsPackage"
            genModel="model/objects.genmodel"
            uri="http://www.eclipse.org/ecl/platform/objects.ecore">
     </package>
     <package
            class="org.eclipse.rcptt.ecl.platform.commands.CommandsPackage"
            genModel="model/commands.genmodel"
            uri="http://www.eclipse.org/ecl/platform/commands.ecore">
      </package>
     
   </extension>
   <extension
         point="org.eclipse.rcptt.ecl.core.scriptlet">
      <scriptlet
            class="org.eclipse.rcptt.ecl.platform.internal.commands.ListPluginsService"
            name="ListPlugins"
            namespace="http://www.eclipse.org/ecl/platform/commands.ecore">
      </scriptlet>
      <scriptlet
            class="org.eclipse.rcptt.ecl.platform.internal.commands.ListFeaturesService"
            name="ListFeatures"
            namespace="http://www.eclipse.org/ecl/platform/commands.ecore">
      </scriptlet>
      <scriptlet
            class="org.eclipse.rcptt.ecl.platform.internal.commands.ListRepositoriesService"
            name="ListRepositories"
            namespace="http://www.eclipse.org/ecl/platform/commands.ecore">
      </scriptlet>
      <scriptlet
            class="org.eclipse.rcptt.ecl.platform.internal.commands.AddRepositoryService"
            name="AddRepository"
            namespace="http://www.eclipse.org/ecl/platform/commands.ecore">
