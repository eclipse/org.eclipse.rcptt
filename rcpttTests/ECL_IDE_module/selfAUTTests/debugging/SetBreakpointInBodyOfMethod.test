------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Q7 Explorer" | get-tree | select "MyProject/CreateJavaProject" | double-click
with [get-editor CreateJavaProject | get-section Script] {
    get-left-ruler | get-ruler-column -index 1 | click-ruler -line 4
    get-text-viewer | key-type "M1+M2+b"
}
exec-with-options -disableJobWaiting -command {
	get-menu "Run/Debug As/\\d+ Test Cases" | click
	try -times 100 -delay 100 -command {
		with [get-view Debug | get-tree] {
    get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread" | get-property childCount | equals 2 
        | verify-true
    get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread/get-button \\(CreateJavaProject: 4\\)" 
        | get-property caption | equals "get-button (CreateJavaProject: 4)" | verify-true
    get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread/with \\(CreateJavaProject: 2\\)" 
        | get-property caption | equals "with (CreateJavaProject: 2)" | verify-true
}
	}
	get-view Debug | get-button "Step Into (F5)" | click
	try -times 100 -delay 100 -command {
		get-view Debug | get-tree | get-item "CreateJavaProject \\[Q7 Test Suite\\]/CreateJavaProject/ECL Thread/click \\(CreateJavaProject: 4\\)" 
		    | get-property caption | equals "click (CreateJavaProject: 4)" | verify-true
	}
	try -times 100 -delay 100 -command {
		get-view Debug | get-button "Resume (F8)" | click
	}
}
